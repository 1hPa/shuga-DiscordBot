{
  "name": "shuga-discordbot",
  "version": "1.0.0",
  "description": "This is a DiscordBot.",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "commitlint": "commitlint -f HEAD",
    "cz": "git-cz"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1hPa/shuga-DiscordBot.git"
  },
  "bugs": {
    "url": "https://github.com/1hPa/shuga-DiscordBot/issues"
  },
  "homepage": "https://github.com/1hPa/shuga-DiscordBot#readme",
  "devDependencies": {
    "@commitlint/cli": "^10.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5"
  },
  "dependencies": {}
}
